<!DOCTYPE html>
<html lang="en">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>PARI/GP in Browser</title>
</head>
<body>

<td class="main">
<h1 class="head">Run PARI/GP in your browser
</h1>
Type a GP expression in the blue box then hit
<code>&lt;Shift&gt;-&lt;Enter&gt;</code> or click on the
<code>Evaluate</code> button below it. A trailing semi-colon <code>;</code>
prevents output, but not warnings or diagnostics.
You can assign to variables and define user functions. Speed is approximately
15% of native speed.
<div id="gpshell">
<pre class="gpoutput" id="output"></pre>
<textarea class="code input" onkeydown="return shiftenter(this,event)"></textarea>
</div>
<button type="button" id="loading" disabled onclick="return eval(this)">Loading PARI</button>
<button type="button" onclick="return clearcell(this)">Clear input</button>
<script type="text/javascript">
function addtext(t) {
&nbsp; var nodeList = document.getElementsByClassName("input");
&nbsp; nodeList[nodeList.length-1].value += t.innerHTML + "\n";
}
</script>
<button type="button" onclick="return newcell(this)">New Cell</button>
<script type="text/javascript">
document.getElementById("loading").setAttribute("disabled",true);
var currOutput = document.getElementById("output");
function cvtohtml(s) {
&nbsp; s = s.replace(/&/g, "&amp;");
&nbsp; s = s.replace(/</g, "&lt;");
&nbsp; s=s.replace( />/g, "&gt;");
&nbsp; s = s.replace(/\n/g, "<br>");
&nbsp; return s;
}
var rawPrint = false;
function myprint(text)
{
&nbsp; if (rawPrint)
&nbsp; {
&nbsp;&nbsp;&nbsp; currOutput.innerHTML += text+"\n";
&nbsp; }
&nbsp; else
&nbsp; {
&nbsp;&nbsp;&nbsp; currOutput.innerHTML += cvtohtml(text)+"\n";
&nbsp; }
}
function myprinterr(text)
{
&nbsp; currOutput.innerHTML += "<span style=\"color: red\">" + cvtohtml(text) + "</span>\n";
}
var initonce = false;
function mystart(where)
{
&nbsp; myinit(); myplotinit(); initonce = true;
&nbsp; var b = document.getElementById("loading");
&nbsp; b.removeAttribute("disabled");
&nbsp; b.innerHTML = "Evaluate with PARI";
}
var Module =
{
&nbsp; noInitialRun: true,
&nbsp; print: myprint,
&nbsp; printErr: myprinterr,
&nbsp; onRuntimeInitialized: mystart,
&nbsp; wasmBinaryFile: 'https://cdn.jsdelivr.net/gh/wangyenshu/wangyenshu.github.io@raw/refs/heads/main/gpwasm/gp-sta.wasm'
};
function myevalm(s) { return Module.ccall("gp_embedded",null,["string"],[s]); }
function myinit() { Module.ccall("pari_emscripten_init",null,["number","number"],[4000000,1000000000]); }
function myplotinit() { Module.ccall("pari_emscripten_plot_init",null,["number","number"],[640,480]); }
function searchid(node, id)
{
&nbsp; var children = node.childNodes;
&nbsp; for (i in children)
&nbsp; {
&nbsp;&nbsp;&nbsp; if (children[i].id == id) { return children[i]; }
&nbsp; }
}
function searchclass(node, id)
{
&nbsp; var children = node.childNodes;
&nbsp; for (i in children)
&nbsp; {
&nbsp;&nbsp;&nbsp; if (children[i].className == id) { return children[i]; }
&nbsp; }
}
function eval(where) {
&nbsp; if (initonce)
&nbsp; {
    var currentCell = where.parentNode.querySelector("#gpshell");
    if (!currentCell) {
        currentCell = document.getElementById("gpshell");
    }
    var x = currentCell.querySelector(".code.input").value;
    currOutput = currentCell.querySelector(".gpoutput#output");
&nbsp;&nbsp;&nbsp; currOutput.innerHTML += "<div style=\"color: blue\">? " + cvtohtml(x) + "</a></div>";
&nbsp;&nbsp;&nbsp; myevalm(x);
&nbsp; }
}
function clearcell(where) {
    var currentCell = where.parentNode.querySelector("#gpshell");
    if (!currentCell) {
        currentCell = document.getElementById("gpshell");
    }
    currentCell.querySelector(".code.input").value = "";
}
function shiftenter(where,e) {
&nbsp;&nbsp; if (e.shiftKey && e.keyCode == 13)
&nbsp;&nbsp; {
    var evaluateButton = document.getElementById("loading");
    if (evaluateButton) {
        eval(evaluateButton);
    }
&nbsp;&nbsp;&nbsp;&nbsp; return false;
&nbsp;&nbsp; }
}
function newcell(where) {
&nbsp; var main = document.getElementById("gpshell");
&nbsp; var clone = main.cloneNode(true);
&nbsp; where.parentNode.insertBefore(clone, where);
&nbsp; searchid(clone,"output").innerHTML = "";
&nbsp; var ci = searchclass(clone,"code input");
&nbsp; ci.value = "";
&nbsp; ci.focus();
}
</script>
<script async type="text/javascript" src="https://cdn.jsdelivr.net/gh/wangyenshu/wangyenshu.github.io@refs/heads/main/gpwasm/gp-sta.js"></script>
</body>
</html>
